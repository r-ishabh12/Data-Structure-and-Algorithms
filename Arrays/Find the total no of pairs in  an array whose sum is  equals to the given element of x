public class Array{
static void totalPairs(int[] arr, int x){
int count = 0;

for (int i = 0; i < arr.length; i++){
    for (int j = i+1; j < arr.length; j++){
if(arr[i] + arr[j] == x){
System.out.println(count+"--------");
System.out.println("{" + arr[i] + ", ");
System.out.println(arr[j]+"}");
}
}
}
}
}
