public class Array{
  static void printArray(int[] arr){
    for(int i = 0; i < arr.length; i++){
      System.out.println(arr[i]+" ");
      }
    }
  static void swapArray(int[] arr, int i, int j){
    int temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
    }
  static void reverseArrayBySwapping(int[] arr){
    int n = arr.length;
    int i = 0;
    int j = n - 1;
    while(i <= j){
      swapArray(arr, i, j);
      i++;
      j--;
      }
    printArray(arr);
    }
  }
      
